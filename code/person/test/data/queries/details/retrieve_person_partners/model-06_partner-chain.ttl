@prefix gx: <http://gedcomx.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/> .

#  +-ex:P1 {typed person}
#  |     (m)
#  |
# [P] {stated}
#  |
#  |
#  +-ex:P2 {typed person}
#  |     (f)
#  |
# [P] {stated}
#  |
#  |
#  +-ex:P3 {typed person}
#  |     (m)
#  |
# [P] {stated}
#  |
#  |
#  +-ex:P4 {typed person}
#        (f)

ex:P1 a gx:Person ;
    gx:gender [ a gx:Gender ; gx:type gx:Male ] .

ex:P2 a gx:Person ;
    gx:gender [ a gx:Gender ; gx:type gx:Female ] .

ex:P3 a gx:Person ;
    gx:gender [ a gx:Gender ; gx:type gx:Male ] .

ex:P4 a gx:Person ;
    gx:gender [ a gx:Gender ; gx:type gx:Female ] .

ex:R1 a gx:Relationship ;
    gx:type gx:Couple ;
    gx:person1 ex:P1 ;
    gx:person2 ex:P2 .

ex:R2 a gx:Relationship ;
    gx:type gx:Couple ;
    gx:person1 ex:P2 ;
    gx:person2 ex:P3 .

ex:R3 a gx:Relationship ;
    gx:type gx:Couple ;
    gx:person1 ex:P3 ;
    gx:person2 ex:P4 .
