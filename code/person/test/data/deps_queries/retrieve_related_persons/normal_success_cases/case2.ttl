@prefix gx: <http://gedcomx.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/> .


ex:P1 a gx:Person ;
    gx:gender [ a gx:Gender ; gx:type gx:Male ] ;
    gx:name [
        gx:type gx:BirthName ;
        gx:preferred "true"^^xsd:boolean ;
        gx:nameForm [
            gx:lang "pl" ;
            gx:part [
                gx:type gx:Given ;
                gx:value "Jan" ] ;
            gx:part [
                gx:type gx:Surname ;
                gx:value "Kowalski"  ] ] ] ;
    gx:birthDate "1970-05-12"^^xsd:date .


ex:P2 a gx:Person ;
    gx:gender [ a gx:Gender ; gx:type gx:Female ] ;
    gx:name [
        gx:type gx:BirthName ;
        gx:preferred "true"^^xsd:boolean ;
        gx:nameForm [
            gx:lang "pl" ;
            gx:part [
                gx:type gx:Given ;
                gx:value "Edyta" ] ;
            gx:part [
                gx:type gx:Surname ;
                gx:value "Malinowska"  ] ] ] ;
    gx:birthDate "1968-11-22"^^xsd:date .


ex:R1 a gx:Relationship ;
    gx:type gx:Couple ;
    gx:person1 ex:P1 ;
    gx:person2 ex:P2 .


ex:R1 a gx:Relationship ;
    gx:type gx:Couple ;
    gx:person1 ex:P1 ;
    gx:person2 ex:P2 .


ex:R2 a gx:Relationship ;
    gx:type gx:Couple ;
    gx:person1 ex:P1 ;
    gx:person2 ex:P2 .
